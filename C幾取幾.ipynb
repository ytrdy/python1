   "source": [
    "a=int(input(\"請輸入物品種類數:\"))\n",
    "b=int(input(\"請輸入欲取出數量:\"))\n",
    "\n",
    "i=b+1\n",
    "C=1\n",
    "\n",
    "while i<=a:\n",
    "    C=C*i\n",
    "    i=i+1\n",
    "\n",
    "k=1\n",
    "\n",
    "while k<=b:\n",
    "    C=C/k\n",
    "    k=k+1\n",
    "\n",
    "print(\"共有%d種組合\"%C)"
