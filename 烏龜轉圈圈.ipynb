     "output_type": "execute_result"
    }
   ],
   "source": [
    "tu.shape(\"turtle\")\n",
    "tu.color(\"green\")\n",
    "i=100\n",
    "while i>=0:\n",
    "    tu.begin_fill()\n",
    "    tu.forward(i)\n",
    "    tu.right(90)\n",
    "    i=i-5\n",
    "    tu.end_fill()\n",
    "tu.done"
